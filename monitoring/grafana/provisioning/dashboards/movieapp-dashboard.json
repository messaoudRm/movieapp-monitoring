{
  "id": null,
  "uid": "movieapp-http",
  "title": "MovieApp - HTTP Metrics",
  "tags": ["spring", "http", "prometheus", "movieapp"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Taux de requêtes HTTP (succès / erreurs)",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"2..\"}[1m]))",
          "legendFormat": "Succès (2xx)",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"4..\"}[1m]))",
          "legendFormat": "Erreurs client (4xx)",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[1m]))",
          "legendFormat": "Erreurs serveur (5xx)",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Temps de réponse moyen (s)",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_sum[1m])) / sum(rate(http_server_requests_seconds_count[1m]))",
          "legendFormat": "Temps moyen global",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    }
  ]
}
